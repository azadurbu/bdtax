      <button onclick="set_no('Accommodation')" type="button" class="btn btn-big btn-<?=($model->AccommodationConfirm == 'No')? 'success' : 'default' ?>"><?=Yii::t("common","NO")?></button>
    </div>
  </h3>
  <!-- END YES/NO BUTTON -->
</div>
